<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stappco.password.sesamo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SettingsActivity"></activity>
        <activity
            android:name=".SetPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WelcomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OthersActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WebsitesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WifiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CommerceActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BankActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EmailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SocialMediaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>